RALEWAY_API_TOKEN=your_raleway_token

# Google Gemini API Configuration
# Get your API key from https://aistudio.google.com/app/apikey
GEMINI_API_KEY=YOUR_GEMINI_API_KEY

# Pinata IPFS Configuration
# Get your keys from https://app.pinata.cloud/developers/api-keys
PINATA_API_KEY=YOUR_PINATA_API_KEY
PINATA_SECRET_API_KEY=YOUR_PINATA_SECRET_API_KEY
PINATA_JWT=YOUR_PINATA_JWT

# Network Configuration
# Options: local, passet, rococo, kusama
# Use 'passet' for Passet Hub (official ink! v6 testnet - recommended)
NETWORK=passet

# Contract Addresses
# Fill in after deploying to each network
PASSET_CONTRACT_ADDRESS=
ROCOCO_CONTRACT_ADDRESS=
KUSAMA_CONTRACT_ADDRESS=
